{
  "name": "powerkernel/yii-core-api",
  "description": "Yii Core API",
  "type": "yii2-module",
  "keywords": [
    "yii2",
    "core",
    "api"
  ],
  "license": "Private",
  "authors": [
    {
      "name": "Harry Tang",
      "email": "harry@powerkernel.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "3.0.x-dev"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "powerkernel\\yiicore\\": "src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}